<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <% provide(:title, 'All users') %>
  <h1>All users</h1>

  <tbody>
    <%= @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>

    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
